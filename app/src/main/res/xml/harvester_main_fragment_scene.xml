<?xml version="1.0" encoding="utf-8"?>
<MotionScene xmlns:motion="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <Transition
        motion:constraintSetEnd="@layout/harvester_camera_fragment_motion_end"
        motion:constraintSetStart="@layout/harvester_camera_fragment_motion_start"
        motion:autoTransition="animateToStart"
        motion:duration="200"
        motion:staggered="0.1">

        <OnSwipe motion:dragDirection="dragDown"
            motion:onTouchUp="autoCompleteToStart"
            motion:maxVelocity="20"
            motion:maxAcceleration="20"/>

    </Transition>

</MotionScene>